# Backend .env template
DEBUG=false
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000
# API Key Authentication (OPTIONAL)
# ====================================
# If set, API keys are required for endpoint access
# If empty or not set, API key authentication is disabled (public access)
# Format: comma-separated list of valid API keys
# Example: API_KEYS="demo-key-1,demo-key-2,prod-key-xyz"
API_KEYS=

# Rate Limiting
# Max requests per minute per API key (default: 60)
RATE_LIMIT_PER_MINUTE=60